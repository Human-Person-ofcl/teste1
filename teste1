local Button = Tab:CreateButton({ Name = "Destrancar Carro üîëN√ÉO USAR", Callback = function() 
	print("By Rodrigues Menu")
	-- Gui to Lua
	-- Version: 3.2

	-- Instances:

	local ScreenGui = Instance.new("ScreenGui")
	local Frame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local TextButton = Instance.new("TextButton")

	--Properties:

	ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Frame.Parent = ScreenGui
	Frame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Frame.BorderSizePixel = 0
	Frame.Position = UDim2.new(0.461669505, 0, 0.457364351, 0)
	Frame.Size = UDim2.new(0, 90, 0, 65)

	UICorner.Parent = Frame

	TextButton.Parent = Frame
	TextButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
	TextButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.BorderSizePixel = 0
	TextButton.Position = UDim2.new(-0.00499742292, 0, 0.112561971, 0)
	TextButton.Size = UDim2.new(0, 90, 0, 50)
	TextButton.Font = Enum.Font.PermanentMarker
	TextButton.Text = "UNLOCK"
	TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
	TextButton.TextScaled = true
	TextButton.TextSize = 14.000
	TextButton.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
	TextButton.TextWrapped = true
	TextButton.MouseButton1Down:Connect(function()

		local function encontrarDriveSeatMaisProximo()
			local personagem = game.Players.LocalPlayer.Character
			if not personagem then return nil end

			local humanoide = personagem:FindFirstChildOfClass("Humanoid")
			if not humanoide then return nil end


			local driveSeatMaisProximo = nil
			local distanciaMinima = math.huge


			for _, veiculo in ipairs(workspace:GetDescendants()) do
				if veiculo:IsA("VehicleSeat") then
					local distancia = (veiculo.Position - personagem.HumanoidRootPart.Position).Magnitude
					if distancia < distanciaMinima then
						driveSeatMaisProximo = veiculo
						distanciaMinima = distancia
					end
				end
			end

			return driveSeatMaisProximo
		end

		-- Fun√ß√£o para ativar propriedades e desmarcar 'Disabled'
		local function ativarPropriedades(driveSeat)
			if driveSeat and driveSeat:IsA("VehicleSeat") then
				driveSeat.CanCollide = true
				driveSeat.CanQuery = true
				driveSeat.CanTouch = true


				if driveSeat.Disabled == true then
					driveSeat.Disabled = false
				end
			else
				warn("Nenhum Carro Proximo")
			end
		end


		local driveSeat = encontrarDriveSeatMaisProximo()
		ativarPropriedades(driveSeat)

	end)


	-- Scripts:

	local function NDWS_fake_script() -- Frame.Script 
		local script = Instance.new('Script', Frame)

		local frame = script.Parent  -- O frame que ser√° arrastado
		local dragging = false
		local dragInput, dragStart, startPos

		local UIS = game:GetService("UserInputService")

		frame.InputBegan:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseButton1 then
				dragging = true
				dragStart = input.Position
				startPos = frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end)

		frame.InputChanged:Connect(function(input)
			if input.UserInputType == Enum.UserInputType.MouseMovement then
				dragInput = input
			end
		end)

		UIS.InputChanged:Connect(function(input)
			if input == dragInput and dragging then
				local delta = input.Position - dragStart
				frame.Position = UDim2.new(
					startPos.X.Scale, startPos.X.Offset + delta.X,
					startPos.Y.Scale, startPos.Y.Offset + delta.Y
				)
			end
		end)

	end
	coroutine.wrap(NDWS_fake_script)()
end })
