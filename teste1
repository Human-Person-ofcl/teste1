	print("Auto-Farm Gari ativado!")
	local teleportLocations = {
		CFrame.new(-346.349335, 11.2034531, -15.1218395, -0.31654954, 0, 0.948575974, 0, 1, 0, -0.948575974, 0, -0.31654954),
